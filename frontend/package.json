{
  "name": "owasp-nest-frontend",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "build": "next build",
    "dev": "next dev --port 3000 --turbo",
    "format": "prettier --log-level warn --write .",
    "format:check": "prettier --check .",
    "graphql-codegen": "graphql-codegen --config graphql-codegen.ts",
