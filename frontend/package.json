{
  "name": "owasp-nest-frontend",
  "private": true,
  "version": "0.0.1",
  "type": "module",
  "scripts": {
    "dev": "vite --host --port 3000",
    "build": "tsc --noEmit && vite build",
    "build:staging": "tsc --noEmit && vite build --mode staging",
    "build:production": "tsc --noEmit && vite build --mode production",
    "format": "prettier --log-level warn --write .",
    "format:check": "prettier --check .",
    "lint": "eslint . --config eslint.config.js --fix --max-warnings=0",
    "lint:check": "eslint . --config eslint.config.js --max-warnings=0",
    "preview": "vite preview",
    "test": "tsc --noEmit && NODE_OPTIONS=--no-warnings=DEP0040 jest",
    "test:coverage": "jest --coverage && chmod +x scripts/open-coverage.sh && scripts/open-coverage.sh",
    "watch": "jest --watch"
  },
  "dependencies": {
    "@algolia/autocomplete-js": "^1.17.9",
    "@algolia/autocomplete-plugin-query-suggestions": "^1.17.9",
    "@algolia/autocomplete-theme-classic": "^1.17.9",
    "@apollo/client": "^3.12.7",
    "@ark-ui/react": "^4.9.1",
    "@chakra-ui/react": "^3.7.0",
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.0",
    "@fortawesome/fontawesome-svg-core": "^6.7.0",
    "@fortawesome/free-brands-svg-icons": "^6.7.0",
    "@fortawesome/free-regular-svg-icons": "^6.7.0",
    "@fortawesome/free-solid-svg-icons": "^6.7.0",
    "@fortawesome/react-fontawesome": "^0.2.2",
    "@radix-ui/react-dropdown-menu": "^2.1.4",
    "@radix-ui/react-toast": "^1.2.4",
    "@sentry/react": "^8.51.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/lodash": "^4.17.14",
    "algoliasearch": "^5.20.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "core-js": "^3.40.0",
    "date-fns": "^4.1.0",
    "dayjs": "^1.11.13",
    "dompurify": "^3.2.3",
    "dotenv": "^16.4.7",
    "graphql": "^16.10.0",
    "leaflet": "^1.9.4",
    "leaflet.markercluster": "^1.5.3",
    "lodash": "^4.17.21",
    "markdown-it": "^14.1.0",
    "millify": "^6.1.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-gtm-module": "^2.0.11",
    "react-icons": "^5.4.0",
    "react-router-dom": "^7.0.2",
    "tailwind-merge": "^3.0.1",
    "tailwindcss-animate": "^1.0.7"
  },
  "devDependencies": {
    "@eslint/js": "^9.15.0",
    "@swc/core": "^1.10.9",
    "@swc/jest": "^0.2.37",
    "@tailwindcss/postcss": "^4.0.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.2.0",
    "@types/date-fns": "^2.6.3",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.10.8",
    "@types/react": "^19.0.6",
    "@types/react-dom": "^19.0.3",
    "@types/react-gtm-module": "^2.0.4",
    "@typescript-eslint/eslint-plugin": "^8.21.0",
    "@typescript-eslint/parser": "^8.21.0",
    "@vitejs/plugin-react": "^4.3.4",
    "autoprefixer": "^10.4.20",
    "eslint": "^9.19.0",
    "eslint-config-prettier": "^10.0.1",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-jest": "^28.9.0",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-react": "^7.37.4",
    "eslint-plugin-react-hooks": "^5.1.0",
    "globals": "^15.14.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "open": "^10.1.0",
    "postcss": "^8.4.47",
    "prettier": "^3.4.2",
    "prettier-plugin-tailwindcss": "^0.6.11",
    "tailwindcss": "^3.4.17",
    "ts-jest": "^29.2.5",
    "ts-node": "^10.9.2",
    "typescript": "~5.7.3",
    "typescript-eslint": "^8.21.0",
    "util": "^0.12.5",
    "vite": "^6.0.11",
    "vite-plugin-environment": "^1.1.3",
    "vite-tsconfig-paths": "^5.1.4"
  },
  "engines": {
    "node": "22"
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {},
      "tailwindcss": {}
    }
  },
  "prettier": {
    "arrowParens": "always",
    "endOfLine": "lf",
    "plugins": [
      "prettier-plugin-tailwindcss"
    ],
    "printWidth": 100,
    "semi": false,
    "singleQuote": true,
    "tabWidth": 2,
    "trailingComma": "es5"
  }
}
